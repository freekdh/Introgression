    "version": "2.0.0",
    "tasks": [
        {
            "label": "build_CSV",
            "type": "shell",
            "command": "g++ -fopenmp -O3 -std=c++11 -I /usr/local/include/boost_1_67_0/ -g ./CppFiles/IntrogressionSimulations.cpp ./CppFiles/IntrogressionSimulations.h ./CppFiles/CSV_output.cpp ./CppFiles/random.cpp ./CppFiles/random.h ./CppFiles/utils.cpp ./CppFiles/utils.h -o ./CppFiles/CSVtest /usr/local/lib/libboost_filesystem.a /usr/local/lib/libboost_system.a",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher":"$gcc"
        },
        {
            "label": "build_RCPP",
            "type": "shell",
            "command": "g++ -fopenmp -O3 -std=c++11 -I /usr/local/include/boost_1_67_0/ -I /usr/local/lib/R/site-library/Rcpp/include/ -I /usr/share/R/include/ -I /usr/local/lib/R/site-library/RcppProgress/include/ -g ./CppFiles/IntrogressionSimulations.cpp ./CppFiles/IntrogressionSimulations.h ./CppFiles/Rcpp_output.cpp ./CppFiles/Rcpp_output.h ./CppFiles/random.cpp ./CppFiles/random.h ./CppFiles/utils.cpp ./CppFiles/utils.h -o RCPPtest",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher":"$gcc"            
        },
        {
            "label": "build_SHINY",
            "type": "shell",
            "command": "g++ -fopenmp -O3 -std=c++11 -I /usr/local/include/boost_1_67_0/ -I /usr/local/lib/R/site-library/Rcpp/include/ -I /usr/share/R/include/ -I /usr/local/lib/R/site-library/RcppProgress/include/ -g ./CppFiles/IntrogressionSimulations.cpp ./CppFiles/IntrogressionSimulations.h ./CppFiles/Shiny_output.cpp ./CppFiles/Rcpp_output.cpp ./CppFiles/Rcpp_output.h ./CppFiles/random.cpp ./CppFiles/random.h ./CppFiles/utils.cpp ./CppFiles/utils.h -o Shinytest",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher":"$gcc"            
        }
    ]
}